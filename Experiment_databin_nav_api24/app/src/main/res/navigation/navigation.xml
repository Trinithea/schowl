<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/navigation" app:startDestination="@id/settingsTestFragment">

    <fragment android:id="@+id/settingsTestFragment"
              android:name="com.example.experiment_databin_nav_api24.SettingsTestFragment"
              android:label="Create test">
        <action android:id="@+id/action_settingsTestFragment_to_testFragment" app:destination="@id/testFragment"/>
    </fragment>
    <fragment
            android:id="@+id/testFragment"
            android:name="com.example.experiment_databin_nav_api24.TestFragment"
            android:label="TestFragment" />
    <fragment android:id="@+id/subjectsFragment"
              android:name="com.example.experiment_databin_nav_api24.SubjectsFragment"
              android:label="SubjectsFragment">
        <action android:id="@+id/action_subjectsFragment_to_categoriesFragment"
                app:destination="@id/categoriesFragment"/>
    </fragment>
    <fragment android:id="@+id/categoriesFragment"
              android:name="com.example.experiment_databin_nav_api24.CategoriesFragment"
              android:label="CategoriesFragment">
        <action android:id="@+id/action_categoriesFragment_to_questionsFragment"
                app:destination="@id/questionsFragment"/>
    </fragment>
    <fragment android:id="@+id/questionsFragment"
              android:name="com.example.experiment_databin_nav_api24.QuestionsFragment"
              android:label="QuestionsFragment">
        <action android:id="@+id/action_questionsFragment_to_editQuestionFragment"
                app:destination="@id/action_edit_category"/>
    </fragment>
    <fragment android:id="@+id/action_edit_category"
              android:name="com.example.experiment_databin_nav_api24.EditQuestionFragment"
              android:label="EditQuestionFragment">
        <action
                android:id="@+id/action_action_edit_category_to_questionsFragment"
                app:destination="@id/questionsFragment" />
    </fragment>
    <fragment
            android:id="@+id/settingsFragment"
            android:name="com.example.experiment_databin_nav_api24.SettingsFragment"
            android:label="SettingsFragment" />
</navigation>